#!/bin/bash


function rmd() {
    rm -fr $1
}


function add-ssh(){
    if [[ -d /home/$USER/.local/crypto/keys/ssh ]];then
        cd /home/$USER/.local/crypto/keys/ssh 
        echo 'i am here'
        if [[ $( ls | wc -l ) > 1 ]];then
            find . -type f  ! -name "*.*" -exec ssh-add {}  \;
        fi
    fi
} 