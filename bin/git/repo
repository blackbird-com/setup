#!/bin/bash

function bb_git_push() {
	
	bb_rbin_head "GITREPO ASSISTANT"

	read -p "Insert github commit message : " COMMIT_MESSAGE
	git add . &&
	if [[ -z $COMMIT_MESSAGE ]];then
		git commit -m "latest update" 
	else
		git commit -m "$COMMIT_MESSAGE"
	fi
	git push
}


function bb_git_clone() {
	git clone $1
}